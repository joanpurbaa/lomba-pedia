generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model lomba {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  nama String
  penyelenggara String
  deadline String
  pendaftaran String
  tingkat String?
  kategori String[]
  peserta String[]
  narahubung String?
  linkPendaftaran String?
  urlPoster String
  poster String?
  deskripsi String
}